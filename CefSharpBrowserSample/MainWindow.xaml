<Window x:Class="CefSharpBrowserSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        xmlns:commonutl="clr-namespace:CefSharpBrowserSample.CommonUtils"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:support="clr-namespace:CefSharpBrowserSample.ViewModel"
        xmlns:local="clr-namespace:CefSharpBrowserSample"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type= {x:Type support:SupportVM}}"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid>
            <cefSharp:ChromiumWebBrowser x:Name="browserHtmlEditor" commonutl:CefSharpBrowserContent.Html="{Binding supportVM.UpdateHistory,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
        </Grid>
    </Grid>
</Window>
